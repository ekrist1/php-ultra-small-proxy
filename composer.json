{
    "name": "szczyglis/php-ultra-small-proxy",
    "description": "Lightweight proxy with full support for sessions, cookies, POST/FORM submissions, and URL rewriting.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/szczyglis-dev/php-ultra-small-proxy",
    "autoload": {
        "psr-4": {
            "Szczyglis\\UltraSmallProxy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Szczyglis\\UltraSmallProxy\\Laravel\\UltraSmallProxyServiceProvider"
            ]
        }
    },
    "keywords": [
        "proxy",
        "client",
        "web",
        "networking",
        "website",
        "webproxy",
        "crawler",
        "rewrite",
        "http",
        "cookies"
    ],
    "authors": [
        {
            "name": "szczyglis",
            "email": "szczyglis@protonmail.com",
            "homepage": "https://szczyglis.dev"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.4",
        "ext-curl": "*",
        "ext-xml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.2"
    }
}
